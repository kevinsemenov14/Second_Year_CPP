#include "header.h"


void Size(personNode *head)
{
	personNode *perPtr;
	int count;
	perPtr = head;


	while(perPtr != NULL)
	{
		count++;
		perPtr = perPtr -> next;
	}


	if(head == NULL)
	{
		cout << "Nobody is on the stack!\n";
	}
}
